{
  "name": "gigsmash",
  "version": "1.0.0",
  "description": "Mashup of North Pembs gigs",
  "scripts": {
    "start": "node server.js",
    "test": "start /b node server.js 7000 & timeout 2 & node --test",
    "deploy": "serverless deploy",
    "deploy:function": "serverless deploy function -f",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:aws": ".\\deploy-aws.bat dev",
    "deploy:aws:dev": ".\\deploy-aws.bat dev",
    "deploy:aws:prod": ".\\deploy-aws.bat prod",
    "install:aws": "npm install && npm install --force --no-save @img/sharp-linux-x64 @img/sharp-libvips-linux-x64",
    "logs": "serverless logs -f",
    "invoke": "serverless invoke -f",
    "invoke:local": "serverless invoke local -f",
    "remove": "serverless remove",
    "info": "serverless info",
    "offline": "serverless offline"
  },
  "engines": {
    "node": ">=22"
  },
  "overrides": {
    "fast-xml-parser": ">=5.3.8"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.650.0",
    "@aws-sdk/client-s3": "^3.650.0",
    "dotenv": "^17.2.3",
    "html-dom-parser": "^5.0.13",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "sharp": "^0.34.5",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "serverless": "^4.20.2",
    "serverless-offline": "^14.4.0"
  }
}
